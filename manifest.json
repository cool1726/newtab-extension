{
    "name": "BlackOn NewTab",
    "version": "1.2",
    "description": "A New Tab extensions with a customized bookmarks and searching.",
    "manifest_version": 3,
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "bookmarks", "storage", "favicon"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
        {
        "resources": ["_favicon/*"],
        "matches": ["<all_urls>"],
        "extension_ids": ["*"]
        }
    ]
  }