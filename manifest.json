{
    "name": "BlackOn NewTab",
    "version": "1.1",
    "description": "A New Tab extensions with a customized bookmarks and searching.",
    "manifest_version": 2,
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "geolocation",
        "bookmarks",
        "<all_urls>"
    ]
  }